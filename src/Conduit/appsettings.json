{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "frontend": {
        "ClusterId": "frontend",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "frontend": {
        "Destinations": {
          "cra": {
            "Address": "http://localhost:3000"
          }
        }
      }
    }
  }
}
